{
    "name": "hawc-frontend",
    "license": "MIT",
    "version": "1.0.0",
    "description": "HAWC frontend code",
    "scripts": {
        "clean": "rimraf ../hawc/static/bundles/* && rimraf ../hawc/webpack-stats.json",
        "build": "npm run clean && webpack --config webpack.config.js",
        "start": "node devServer.js",
        "lint": "eslint --cache .",
        "format": "eslint --cache --fix .",
        "test": "./node_modules/mochapack/bin/mochapack \"tests/**/*.js\"",
        "test-windows": "node_modules\\.bin\\mochapack \"tests/**/*.js\"",
        "generate-parsers": "peggy ./shared/parsers/query.pegjs && peggy ./shared/parsers/format.pegjs"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shapiromatron/hawc.git"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.6",
        "@babel/core": "^7.22.8",
        "@babel/generator": "^7.22.7",
        "@babel/parser": "^7.22.7",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.22.7",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.22.7",
        "@babel/preset-react": "^7.22.5",
        "@babel/runtime": "^7.22.6",
        "assert": "^2.0.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.3.0",
        "case-sensitive-paths-webpack-plugin": "^2.4.0",
        "css-loader": "~6.8.1",
        "eslint": "^6.8.0",
