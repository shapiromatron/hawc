        "react": "^18.2.0",
        "react-autosuggest": "^10.1.0",
        "react-datepicker": "~4.16.0",
        "react-dom": "^18.2.0",
        "react-plotly.js": "^2.6.0",
        "react-quill": "~2.0.0",
        "react-tabs": "~4.3.0",
        "slugify": "^1.6.6",
        "sortablejs": "~1.15.0",
        "tablesort": "~5.3.0",
        "textures": "^1.2.3"
