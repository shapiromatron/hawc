{
    "name": "hawc-frontend",
    "license": "MIT",
    "version": "1.0.0",
    "description": "HAWC frontend code",
    "scripts": {
        "clean": "rimraf ../hawc/static/bundles/* && rimraf ../hawc/webpack-stats.json",
        "build": "npm run clean && vite build --config vite.config.js && node vite-manifest-to-webpack-stats.js",
        "start": "vite --config vite.config.js",
        "lint": "eslint --cache .",
        "format": "eslint --cache --fix .",
        "test": "vitest run --config vitest.config.js --exclude tests/helpers.js",
        "test:ui": "vitest --ui --config vitest.config.js --exclude tests/helpers.js",
        "generate-parsers": "peggy ./shared/parsers/query.pegjs && peggy ./shared/parsers/format.pegjs",
        "dev:vite": "vite --config vite.config.js",
        "build:vite": "npm run clean && vite build --config vite.config.js && node vite-manifest-to-webpack-stats.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shapiromatron/hawc.git"
    },
    "devDependencies": {
        "@babel/cli": "^7.22.6",
        "@babel/core": "^7.22.8",
        "@babel/generator": "^7.22.7",
        "@babel/parser": "^7.22.7",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.27.1",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.22.7",
        "@babel/preset-react": "^7.27.1",
        "@babel/runtime": "^7.22.6",
        "@vitest/ui": "^3.2.2",
        "assert": "^2.0.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "express": "^4.20.0",
        "jsdom": "^26.1.0",
        "mocha": "^10.2.0",
        "mochapack": "^2.1.4",
        "peggy": "^4.0.2",
        "prettier": "^1.19.1",
        "rimraf": "^5.0.1",
        "vite": "^4.0.0",
        "vitest": "^3.2.2"
    },
    "dependencies": {
        "clipboard": "~2.0.11",
        "d3": "7.8.5",
        "d3-arrow": "^0.0.25",
        "d3-cloud": "^1.2.5",
        "datatables.net": "^1.13.5",
        "lodash": "^4.17.21",
        "mobx": "^5.15.7",
        "mobx-react": "^6.3.1",
        "moment": "^2.29.4",
        "plotly.js": "3.0.1",
        "prop-types": "^15.8.1",
        "quill": "^1.3.7",
        "react": "^16.14.0",
        "react-autosuggest": "^10.1.0",
        "react-dom": "^16.14.0",
        "react-plotly.js": "^2.6.0",
        "react-quill": "^1.3.5",
        "react-tabs": "~4.3.0",
        "slugify": "^1.6.6",
        "sortablejs": "~1.15.0",
        "tablesort": "~5.3.0",
        "textures": "^1.2.3",
        "velocity-react": "^1.4.3"
    }
}
