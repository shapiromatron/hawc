{
  "name": "hawc-frontend",
  "license": "MIT",
  "version": "1.0.0",
  "description": "HAWC frontend code",
  "type": "module",
  "scripts": {
    "build": "vite build --config vite.config.js",
    "start": "vite --config vite.config.js",
    "lint": "biome check --error-on-warnings .",
    "format": "biome check --write .",
    "test": "vitest run --config vitest.config.js --exclude tests/helpers.js",
    "generate-parsers": "peggy ./shared/parsers/query.pegjs --format es && peggy ./shared/parsers/format.pegjs --format es"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shapiromatron/hawc.git"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@biomejs/biome": "2.3.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.0.1",
    "@vitejs/plugin-react": "^5.1.1",
    "@types/react": "^18.3.27",
    "assert": "^2.0.0",
    "jsdom": "^27.2.0",
    "peggy": "^4.0.2",
    "vite": "^7.2.4",
    "vite-plugin-externals": "^0.6.2",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "clipboard": "~2.0.11",
    "d3": "7.9.0",
    "d3-arrow": "^0.0.25",
    "d3-cloud": "^1.2.5",
    "datatables.net": "^2.3.5",
    "lodash": "^4.17.21",
    "mobx": "^6.15.0",
    "mobx-react": "^7.6.0",
    "moment": "^2.29.4",
    "plotly.js": "3.3.0",
    "prop-types": "^15.8.1",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^18.3.1",
    "react-plotly.js": "^2.6.0",
    "react-quill": "^2.0.0",
    "react-tabs": "~6.1.0",
    "slugify": "^1.6.6",
    "sortablejs": "~1.15.0",
    "tablesort": "~5.6.0",
    "textures": "^1.2.3"
  }
