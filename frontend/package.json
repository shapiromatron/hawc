        "react-dom": "^16.14.0",
        "react-plotly.js": "^2.6.0",
        "react-quill": "^1.3.5",
        "react-tabs": "~4.3.0",
        "slugify": "^1.6.6",
        "sortablejs": "~1.15.0",
