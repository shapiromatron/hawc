{
    "rules": {
        "generator-star-spacing": 1,
        "linebreak-style": [2, "unix"],
        "no-await-in-loop": 1,
        "no-console": 1,
        "no-sparse-arrays": 2,
        "no-unused-vars": [2, { "args": "none" }],
        "object-shorthand": 1,
        "prettier/prettier": "error",
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/no-deprecated": 1,
        "react/prop-types": 1,
        "react/react-in-jsx-scope": 2
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 9,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "mocha": true,
        "es6": true,
        "node": true,
        "jquery": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "plugins": [
        "babel",
        "react",
        "prettier"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
